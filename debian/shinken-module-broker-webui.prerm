#!/bin/bash
#DEBHELPER#

set -e

case "$1" in
  remove|upgrade|deconfigure)

    # Delete manually this file ...
    rm /usr/lib/python2.7/dist-packages/shinken/modules/webui_broker/__init__.pyc || true

    rm /usr/lib/python2.7/dist-packages/shinken/webui/htdocs || true
    rm /usr/share/shinken/htdocs/js/jquery.js || true
    rm /usr/share/shinken/htdocs/font/FontAwesome.otf || true
    rm /usr/share/shinken/htdocs/font/fontawesome-webfont.ttf || true
  ;;
  failed-upgrade)
  ;;
  *)
    echo "prerm called with unknown argument \`$1'" >&2
    exit 1
  ;;
esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0

